{
  "manifest_version": 2
, "name": "Github collapse files"
, "version": "0.0.1"
, "description": "Adds a 'collapse' button to files in PRs"
, "permissions": [
    "https://github.com/",
    "cookies",
    "storage",
    "unlimitedStorage",
    "background"]
, "content_scripts": [
    {
      "matches": ["https://github.com/*"]
    , "js": ["lib/jquery-1.8.2.js","lib/underscore.js","lib/underscore_mixins.js","collapse.js"]
    }
  ]
}
